{
    "name": "noia-master",
    "version": "1.0.1",
    "description": "NOIA Master Node Software Suite.",
    "repository": "git@github.com:noia-network/noia-master.git",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "start": "node -r ts-node/register/transpile-only src/index.ts",
        "build": "tsc",
        "pretest": "tsc -p . --noEmit",
        "test": "npm run tslint && echo Warning: jest tests are skipped.",
        "posttest": "npm run prettier",
        "watch": "tsc -w",
        "tslint": "tslint --project . --config ./tslint.json && echo Successfully passed tslint test.",
        "prettier": "prettier \"src/**/*\" -l"
    },
    "keywords": [],
    "author": {
        "name": "NOIA Network Limited"
    },
    "license": "LGPL-2.1",
    "devDependencies": {
        "@types/jest": "^24.0.22",
        "jest": "24.9.0",
        "chai": "4.2.0",
        "mocha": "6.2.2",
        "prettier": "1.18.2",
        "simplr-tslint": "1.0.0-alpha.14",
        "ts-jest": "24.1.0",
        "ts-node": "8.4.1",
        "tslint": "5.11.0",
        "typescript": "3.7.2"
    },
    "dependencies": {
        "@noia-network/protocol": "1.3.0",
        "@noia-network/webrtc-direct-client": "1.0.1-rc.2",
        "@types/crypto-js": "3.1.43",
        "@types/express": "4.16.0",
        "@types/fs-extra": "5.0.4",
        "@types/geoip-lite": "1.1.29",
        "@types/lokijs": "1.5.2",
        "@types/node": "12.12.11",
        "@types/node-fetch": "2.1.2",
        "@types/ping": "0.2.0",
        "@types/request": "2.47.1",
        "@types/request-promise-native": "1.0.15",
        "@types/vorpal": "1.11.0",
        "@types/ws": "6.0.0",
        "bignumber.js": "2.0.7",
        "chalk": "2.4.1",
        "create-torrent": "3.32.1",
        "crypto-js": "3.1.9-1",
        "density-clustering": "1.3.0",
        "express": "4.16.4",
        "fs-chunk-store": "1.7.0",
        "fs-extra": "7.0.0",
        "geoip-lite": "1.3.2",
        "geolib": "2.0.24",
        "getdomain": "1.3.1",
        "https": "1.0.0",
        "inherits": "2.0.1",
        "ip": "1.0.1",
        "ip-to-int": "0.3.1",
        "ipfs": "0.29.3",
        "lokijs": "1.5.5",
        "node-fetch": "2.2.0",
        "once": "1.3.0",
        "parse-torrent": "5.9.1",
        "ping": "0.2.2",
        "protobufjs": "6.8.8",
        "request": "2.88.0",
        "request-promise-native": "1.0.5",
        "sha1": "1.1.1",
        "strict-event-emitter-types": "2.0.0",
        "tslib": "1.9.3",
        "vorpal": "1.12.0",
        "wrtc": "0.2.1",
        "ws": "6.0.0"
    },
    "files": [
        "dist",
        "**/*.md",
        "@types",
        "!/examples"
    ],
    "jest": {
        "transform": {
            "^.+\\.(ts|tsx)?$": "ts-jest"
        },
        "testEnvironment": "node",
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
